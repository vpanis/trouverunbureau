= render 'shared/menu_bar', user: current_represented
= render 'shared/profile_menu_bar', user: current_user, selected: 'venues'

#venue.main-content.dashboard-form.clearfix.overflow-hidden
  = render 'shared/edit_venue_progress', venue: @venue, progress: 60, step: 5
  .details-section.col-md-7.col-md-offset-1.margin-top
    - case @collection_method
    - when "mangopay"
      = render 'mangopay_payout_methods'
    - when "braintree"
      = render 'braintree_payout_methods'
